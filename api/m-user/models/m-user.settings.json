{
  "kind": "collectionType",
  "collectionName": "m_users",
  "info": {
    "name": "MUser",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "teams": {
      "via": "owner",
      "collection": "team"
    },
    "user_teams": {
      "collection": "user-team",
      "via": "mUser"
    },
    "firstName": {
      "type": "string",
      "required": false,
      "maxLength": 45
    },
    "secondName": {
      "type": "string",
      "maxLength": 45
    },
    "lastName": {
      "type": "string",
      "required": false,
      "unique": false,
      "maxLength": 45
    },
    "phoneNumber": {
      "type": "string",
      "unique": true,
      "maxLength": 12,
      "minLength": 12
    },
    "usersPermissionsUser": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "user_chats": {
      "via": "mUser",
      "collection": "user-chat"
    }
  }
}
