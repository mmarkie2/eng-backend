{
  "kind": "collectionType",
  "collectionName": "user_chats",
  "info": {
    "name": "UserChat"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "inviteDate": {
      "type": "datetime",
      "required": true
    },
    "startDate": {
      "type": "datetime"
    },
    "stopDate": {
      "type": "datetime"
    },
    "mUser": {
      "via": "user_chats",
      "model": "m-user"
    },
    "chat": {
      "model": "chat",
      "via": "chatUsers"
    }
  }
}
